{
  "name": "@randajan/group-map",
  "version": "2.0.0",
  "description": "Lightweight two-level Map — group → key → value — with a tiny, chainable API.",
  "repository": "randajan/group-map",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    "./map": {
      "import": "./dist/esm/MapMap.mjs",
      "require": "./dist/cjs/MapMap.cjs"
    },
    "./set": {
      "import": "./dist/esm/MapSet.mjs",
      "require": "./dist/cjs/MapSet.cjs"
    },
    "./array": {
      "import": "./dist/esm/MapArray.mjs",
      "require": "./dist/cjs/MapArray.cjs"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@randajan/simple-lib": "^3.3.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "map",
    "nested-map",
    "dual-key",
    "group-map",
    "two-level-map",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/randajan/group-map",
